      "version": "0.2.0",
      "configurations": [

        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\push_swap\\src\\a.out",
            "MIMode": "gdb",
            "cwd": "${workspaceFolder}",
            "args": ["5", "1", "14", "15", "2", "7", "8", "10", "19", "20", "18", "12", "3", "11", "9", "17", "13", "4", "6", "16"],
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ],
}
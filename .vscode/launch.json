      "version": "0.2.0",
      "configurations": [

        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\push_swap\\src\\a.out",
            "MIMode": "gdb",
            "cwd": "${workspaceFolder}",
            "args": ["15",  "19",  "9",  "4",  "6",  "1",  "8",  "16",  "12",  "11",  "2",  "18",  "10",  "17",  "14",  "20",  "5",  "13",  "3",  "7"],
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ],

}